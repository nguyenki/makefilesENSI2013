list.txt:	list1.txt list2.txt list3.txt list4.txt list5.txt  
	cp list1.txt list.txt ; cat list2.txt >> list.txt ; cat list3.txt >> list.txt ; cat list4.txt >> list.txt ; cat list5.txt >> list.txt ;

